# Unit Test List
units = [max_num_unit, min_num_unit, sum_elems_unit, count_elem_unit, remove_val_unit, insert_at_unit,
         elim_dups_unit, exchange_elems_unit, reverse_unit, greatest_sum_unit, dot_product_unit,
         mean_unit, mode_unit, perimeter_unit, prime_list_unit, sum_sieve_unit, prime_factorization_unit,
         flatten_unit, partition_unit, open_lockers_unit, longest_monotonic_unit, sublist_sum_unit,
         mnk_won_unit, bubble_sort_unit, selection_sort_unit, median_unit]

# Create Unit Test
unit_test = UnitTest("Lists and Tuples Unit Test", units)

# Run Test!
unit_test.run_test()
